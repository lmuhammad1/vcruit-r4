<div class="breadcrumb-wrapper">

<% if current_user_profile.nil? %>
  <p id="notice"><%= notice %></p>
  <h2>You must complete your user profile to continue</h2>
<% else %>
  <p id="notice"><%= notice %></p>
  <div class="navbar">
    <div class="navbar-inner">
      <ul class="nav">
        <li><%= link_to "Video Hub",   video_pages_path %></li>
        <li><%= link_to 'My Profile', profile_path(current_user.profile) %></li>
        <li><%= link_to "Making a Video",   video_pages_path %></li>
      </ul>
    </div>
  </div>
<h2 class="welcome-message">Welcome, <%= current_user_profile.nil? ? current_user.email : current_user_profile.first_name %>!</h2>
 <% end %>
</div>
